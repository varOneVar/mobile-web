{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run lint && vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix ./src",
    "svgo:color": "svgo -f src/icons/svg-color",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.8.25",
    "fuse.js": "^5.2.3",
    "is-online": "^8.3.0",
    "localforage": "^1.7.3",
    "mand-mobile": "^2.5.9",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "http-proxy": "^1.18.0",
    "postcss-pxtorem": "^5.1.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^4.2.5",
    "vue-template-compiler": "^2.6.11",
    "vuex-persistedstate": "^3.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
